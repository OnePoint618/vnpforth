: _enetdown             100     ; ( -- 100, Network is down )
: _enetunreach          101     ; ( -- 101, Network is unreachable )
: _enetreset            102     ; ( -- 102, Network dropped connection because of reset )
: _econnaborted         103     ; ( -- 103, Software caused connection abort )
: _econnreset           104     ; ( -- 104, Connection reset by peer )
: _enobufs              105     ; ( -- 105, No buffer space available )
: _eisconn              106     ; ( -- 106, Transport endpoint is already connected )
