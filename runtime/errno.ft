: _eilseq               84      ; ( --  84, Illegal byte sequence )
: _erestart             85      ; ( --  85, Interrupted sys call should be restarted )
: _estrpipe             86      ; ( --  86, Streams pipe error )
: _eusers               87      ; ( --  87, Too many users )
: _enotsock             88      ; ( --  88, Socket operation on non-socket )
: _edestaddrreq         89      ; ( --  89, Destination address required )
: _emsgsize             90      ; ( --  90, Message too long )
: _eprototype           91      ; ( --  91, Protocol wrong type for socket )
: _enoprotoopt          92      ; ( --  92, Protocol not available )
: _eprotonosupport      93      ; ( --  93, Protocol not supported )
: _esocktnosupport      94      ; ( --  94, Socket type not supported )
: _eopnotsupp           95      ; ( --  95, Operation not supported on transport endpt )
: _epfnosupport         96      ; ( --  96, Protocol family not supported )
: _eafnosupport         97      ; ( --  97, Address family not supported by protocol )
: _eaddrinuse           98      ; ( --  98, Address already in use )
