: _estale               116     ; ( -- 116, Stale NFS file handle )
: _euclean              117     ; ( -- 117, Structure needs cleaning )
: _enotnam              118     ; ( -- 118, Not a XENIX named type file )
: _enavail              119     ; ( -- 119, No XENIX semaphores available )
: _eisnam               120     ; ( -- 120, Is a named type file )
: _eremoteio            121     ; ( -- 121, Remote I/O error )
: _edquot               122     ; ( -- 122, Quota exceeded )
: _enomedium            123     ; ( -- 123, No medium found )
: _emediumtype          124     ; ( -- 124, Wrong medium type )
